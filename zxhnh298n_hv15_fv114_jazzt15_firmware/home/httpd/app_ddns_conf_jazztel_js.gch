<script language="javascript">var PARA = new Array("Enable","CustomServer","CustomURL","DomainName","IfName","ProviderName","Password","Username");function getmsg(id){var str = new Array();str[0] = new Array(101, "&?error_101;");str[1] = new Array(102, "&?error_102;");str[2] = new Array(103, "&?error_103;");str[3] = new Array(104, "&?error_104;");str[4] = new Array(114, "&?error_114;");str[5] = new Array(106, "&?error_106;");str[6] = new Array(107, "&?err_ddns_001;");str[7] = new Array(108, "&?err_ddns_002;");str[8] = new Array(109, "&?err_ddns_003;");return getMsgFormArray(str, arguments)}function pageLoad(url){getObj("fSubmit").action = url;var errstr= getValue("IF_ERRORSTR");var errpara = getValue("IF_ERRORPARAM");OldProcessRet(errstr, errpara);moveErrLayer(595,93);pageGetValue();}function pageGetValue(){jslPostToViewCheckBox("Frm_Enable", "Enable");jslSetValue("Frm_Interface", "IfName");jslSetValue("Frm_Name","Username");jslSetValue("Frm_Server", "CustomServer");jslSetValue("Frm_URL", "CustomURL");jslSetValue("Frm_DomainName", "DomainName");jslSetValue("Frm_Username", "ProviderName");jslSetValue("Frm_Password", "Password", "GET");}function pageSetValue(){HiddenParaInit(PARA);jslViewToPostCheckBox("Enable", "Frm_Enable");jslSetValue("IfName", "Frm_Interface");jslSetValue("Username", "Frm_Name");jslSetValue("CustomServer", "Frm_Server");jslSetValue("CustomURL", "Frm_URL");jslSetValue("DomainName", "Frm_DomainName");jslSetValue("ProviderName", "Frm_Username");jslSetValue("Password", "Frm_Password", "SET");}function pageCheckValue(){if(getValue("Frm_Name") != "no-ip"&& getValue("Frm_Name") != "dyndns"){msg = getmsg(107);ShowErrorForCom("Frm_Name", "Fnt_Name", msg);return false;}var value = getValue("Frm_Server");if(checkGenStringForASC(value) != true){msg = getmsg(114);ShowErrorForCom("Frm_Server", "Fnt_Server", msg);return false;}if(checkStrLengthRange(value, 0, 128) == -3){msg = getmsg(102,0,128);ShowErrorForCom("Frm_Server", "Fnt_Server", msg);return false;}if(checkCustomSpace(value) != true){msg = getmsg(108);ShowErrorForCom("Frm_Server","Fnt_Server",msg);return false;}if(value.substr(0,7) == "http:\/\/"){msg = getmsg(109);ShowErrorForCom("Frm_Server","Fnt_Server",msg);return false;}var value = getValue("Frm_URL");if(checkGenStringForASC(value) != true){msg = getmsg(114);ShowErrorForCom("Frm_URL","Fnt_URL",msg);return false;}if(checkStrLengthRange(value, 0, 128) == -3){msg = getmsg(102,0,128);ShowErrorForCom("Frm_URL","Fnt_URL",msg);return false;}if(checkCustomSpace(value) != true){msg = getmsg(108);ShowErrorForCom("Frm_URL","Fnt_URL",msg);return false;}if(checkNull(getValue("Frm_DomainName")) != true){msg = getmsg(101);ShowErrorForCom("Frm_DomainName","Fnt_DomainName",msg);return false;}var value = getValue("Frm_DomainName");if(checkStrLengthRange(value, 1, 256) != true){msg = getmsg(102,1,256);ShowErrorForCom("Frm_DomainName","Fnt_DomainName",msg);return false;}if(checkDomainName(getValue("Frm_DomainName")) != true){msg = getmsg(106);ShowErrorForCom("Frm_DomainName","Fnt_DomainName",msg);return false;}if(checkNull(getValue("Frm_Username")) != true){msg = getmsg(101);ShowErrorForCom("Frm_Username","Fnt_Username",msg);return false;}if(checkGenStringForASC(getValue("Frm_Username")) != true){msg = getmsg(114);ShowErrorForCom("Frm_Username","Fnt_Username",msg);return false;}var value = getValue("Frm_Username");if(checkStrLengthRange(value, 1, 256) != true){msg = getmsg(102,1,256);ShowErrorForCom("Frm_Username","Fnt_Username",msg);return false;}if(checkNull(getValue("Frm_Password")) != true){msg = getmsg(101);ShowErrorForCom("Frm_Password","Fnt_Password",msg);return false;}if(checkGenStringForASC(getValue("Frm_Password")) != true){msg = getmsg(114);ShowErrorForCom("Frm_Password","Fnt_Password",msg);return false;}var value = getValue("Frm_Password");if(checkStrLengthRange(value, 1, 256) != true){msg = getmsg(102,1,256);ShowErrorForCom("Frm_Password","Fnt_Password",msg);return false;}return true;}function checkCustomSpace(str){str = trimLRSpaces(str);var temp = trimAllSpaces(str);if (str != temp){return false;}return true;}function pageCancel(){DisableALL();getObj("fSubmit").submit();}function pageSubmit(){if(pageCheckValue() == true){pageSetValue();setValue("IF_ACTION", "apply");DisableALL();getObj("fSubmit").submit();}}</script>