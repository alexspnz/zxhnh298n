<script language="javascript">var PREFIX_BAN_PORT_PARA = new Array("PortID","PortName","OpFlag");function getmsg(id){var str = new Array();str[0] = new Array(118, "&?error_118;");str[1] = new Array(122, "&?error_122;");return getMsgFormArray(str, arguments) }function pageLoad(url){getObj("fSubmit").action = url;var errstr= getValue("IF_ERRORSTR");var errpara = getValue("IF_ERRORPARAM");OldProcessRet(errstr, errpara);moveErrLayer(580,40);pageGetValue();}function pageGetValue(){var number = getValue("IF_INSTNUM");for(var i = 0; i < number; i++){if(document.getElementById("OpFlag"+ i).value == "1"){setChecked("Frm_PortName"+ i, true);}else {setChecked("Frm_PortName"+ i, false);}}}function pageSetValue(){HiddenParaInit(PREFIX_BAN_PORT_PARA);var number = getValue("IF_INSTNUM");for(var i = 0; i < number; i++){if(document.getElementById("Frm_PortName"+ i).checked == true){setValue("OpFlag"+ i, "1");}else {setValue("OpFlag"+ i, "0");}}}function pageCancel(){DisableALL();getObj("fSubmit").submit();}function pageSubmit(){pageSetValue();setValue("IF_ACTION", "apply");DisableALL();getObj("fSubmit").submit();}</script>