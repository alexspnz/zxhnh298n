if(false == DhcpCheckde){setValue("Delegation", "RA");}else if(true == DhcpCheckde){setValue("Delegation", "RA/DHCPv6");}}}function pageEdit(){if(true == pageCheckValue()){pageSetValue();setValue("IF_ACTION", "apply");setValue("IF_INDEX", index);DisableALL();getObj("fSubmit").submit();}}function back4add(){var number = getValue("IF_INSTNUM");for(var i = 0; i < number; i++){jslDisable("modify"+ i);}DisableALL();getObj("fSubmit").submit();}function pageReFresh(){DisableALL();getObj("fSubmit").submit();}</script>