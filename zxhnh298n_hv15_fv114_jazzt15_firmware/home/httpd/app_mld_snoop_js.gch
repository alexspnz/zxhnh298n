<script>var IGMPSNOOP_PARA = new Array("MldEnable","MldMtoU");function getmsg(id){var str = new Array();str[0] = new Array(118, "&?error_118;");str[1] = new Array(119, "&?error_119;");str[2] = new Array(122, "&?error_122;");return getMsgFormArray(str, arguments)}function pageLoad(url){getObj("fSubmit").action = url;var errstr= getValue("IF_ERRORSTR");var errpara = getValue("IF_ERRORPARAM");OldProcessRet(errstr, errpara);pageGetValue();}function pageGetValue(){var tem1 = getValue("MldEnable");var tem2 = getValue("MldMtoU");if (tem1 == 1){getObj("Frm_Enable").checked = true;}else {getObj("Frm_Enable").checked = false;}DoChange();if (tem2 == 1){getObj("Frm_Enable_increase").checked = true;}else {getObj("Frm_Enable_increase").checked = false;}}function pageSetValue(){HiddenParaInit(IGMPSNOOP_PARA);if (getObj("Frm_Enable").checked == true){setValue("MldEnable",1);}else {setValue("MldEnable",0);}if (getObj("Frm_Enable_increase").checked == true){setValue("MldMtoU",1);}else {setValue("MldMtoU",0);}}function pageCheckValue(){return true;}function pageCancel(){jslDisable("Btn_Submit","Btn_Cancel");getObj("fSubmit").submit();}function pageSubmit(){jslDisable("Btn_Submit","Btn_Cancel");pageSetValue();setValue("IF_ACTION", "apply");getObj("fSubmit").submit();}function DoChange(){if(getChecked("Frm_Enable")==true){jslEnable("Fnt_Enable_increase", "Frm_Enable_increase");}else {setChecked("Frm_Enable_increase", false);jslDisable("Fnt_Enable_increase", "Frm_Enable_increase");}}</script>