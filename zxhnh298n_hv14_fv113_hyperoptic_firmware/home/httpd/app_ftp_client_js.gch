<script language="javascript">var FTP_PARA =["FtpEnable","Username","Password","FtpAnon"];var index = "";function getmsg(id){var str = new Array();str[0] = new Array(118, "&?error_118;");str[1] = new Array(119, "&?error_119;");str[2] = new Array(122, "&?error_122;");str[3] = new Array(102, "&?error_102;");str[4] = new Array(103, "&?error_103;");str[5] = new Array(114, "&?error_114;");return getMsgFormArray(str, arguments);}function pageLoad(url){getObj("fSubmit").action = url;var errstr= getValue("IF_ERRORSTR");if("&?cmret_259;"== errstr){ShowErrorForCom(null, null, errstr);}else if (errstr != "SUCC"){var errpara = getValue("IF_ERRORPARAM");if (errpara != "SUCC"){if (getObj("Fnt"+errpara) == null){msg = getmsg(122);ShowErrorForCom(null, null, msg);}else {msg = getmsg(118, "Fnt"+errpara);ShowErrorForCom("Frm"+errpara, null, msg);}}else {msg = getmsg(122);ShowErrorForCom(null, null, msg);}}pageGetValue();}function pageGetValue(){setChecked("Frm_FtpEnable", parseInt(getValue("FtpEnable")));jslSetValue("Frm_FtpAnon", "FtpAnon");if(getValue("Frm_FtpAnon")=="0"){getObj("name").style.display="";getObj("pwd").style.display="";}else {getObj("name").style.display="none";getObj("pwd").style.display="none";}jslSetValue("Frm_Username", "Username");jslSetValue("Frm_Password", "Password", "GET");}function pageSetValue(){HiddenParaInit(FTP_PARA);if( getChecked("Frm_FtpEnable") == "true"|| getChecked("Frm_FtpEnable") == true)setValue("FtpEnable",1);else setValue("FtpEnable",0);jslSetValue("FtpAnon", "Frm_FtpAnon");if(getValue("Frm_FtpAnon")=="0"){getObj("name").style.display="";getObj("pwd").style.display="";jslSetValue("Username", "Frm_Username");jslSetValue("Password", "Frm_Password", "SET");}else {getObj("name").style.display="none";getObj("pwd").style.display="none";}}function FtpAnon_Choose(){if(getValue("Frm_FtpAnon")=="0"){jslEnable("Frm_Username","Frm_Password");getObj("name").style.display="";getObj("pwd").style.display="";}else {getObj("name").style.display="none";getObj("pwd").style.display="none";}}function pageCheckValue(){var msg = "";if(getValue("Frm_FtpAnon") == "0"){var str = getValue("Frm_Username");if(checkGenStringForASC(str) == false){msg = getmsg(114);ShowErrorForCom("Frm_Username","Fnt_Username",msg);return false;}if(getValue("Frm_FtpAnon")=="0"){if(checkStrLengthRange(str,1,256) != true){msg = getmsg(102,1,256);ShowErrorForCom("Frm_Username","Fnt_Username",msg);return false;}}else {if(checkStr(str,0,256) != true){msg = getmsg(102,1,256);ShowErrorForCom("Frm_Username","Fnt_Username",msg);return false;}}if (str.match("[= ]") != null){msg = getmsg(114);ShowErrorForCom("Frm_Username","Fnt_Username",msg);return false;}str = getValue("Frm_Password");if(checkGenStringForASC(str) == false){msg = getmsg(114);ShowErrorForCom("Frm_Password","Fnt_Password",msg);return false;}if(getValue("Frm_FtpAnon")=="0"){if(checkStrLengthRange(str,1,256) != true){msg = getmsg(102,1,256);ShowErrorForCom("Frm_Password","Fnt_Password",msg);return false;}}else {if(checkStr(str,0,256) != true){msg = getmsg(102,1,256);ShowErrorForCom("Frm_Password","Fnt_Password",msg);return false;}}if (str.match("[= ]") != null){msg = getmsg(114);ShowErrorForCom("Frm_Password","Fnt_Password",msg);return false;}}return true;}function pageSubmit(){if(pageCheckValue() == true){jslDisable("Btn_Submit","Btn_Cancel");pageSetValue();setValue("IF_ACTION", "apply");setValue("IF_INDEX",0);DisableALL();getObj("fSubmit").submit();}else {}}function pageCancel(){jslDisable("Btn_Submit","Btn_Cancel");DisableALL();getObj("fSubmit").submit();}function checkStr(value, min, max){var len = value.length;if((len < min) || (len > max)){return -3;}return true;}</script>